---
const links = [{ name: "Inicio", href: "/" }];
---

<nav>
  <ul class="w-full flex justify-center gap-5">
    {
      links.map((link) => (
        <li>
          <a
            class=" hover:text-white uppercase font-bold px-2 py-1"
            href={link.href}
          >
            {link.name}
          </a>
        </li>
      ))
    }
  </ul>
</nav>
